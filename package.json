{
  "name": "cabs",
  "version": "0.0.1",
  "description": "",
  "author": "perfringis",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./__tests__/jest.config.json",
    "test:script": "jest --config=./__tests__/jest.config.json ./__tests__/entity/CalculateTransitPrice.spec.ts",
    "test:script:debug": "NODE_OPTIONS='-r ts-node/register' node --nolazy --inspect-brk -r tsconfig-paths/register ./node_modules/jest/bin/jest.js --runInBand --colors --verbose --config ./__tests__/jest.config.json ./__tests__/entity/CalculateTransitPrice.spec.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "NODE_OPTIONS='-r ts-node/register' node --nolazy --inspect-brk -r tsconfig-paths/register ./node_modules/jest/bin/jest.js --runInBand --colors --verbose --config ./__tests__/jest.config.json ./__tests__",
    "test:e2e": "jest --config ./e2e/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^10.4.2",
    "@nestjs/platform-express": "^10.4.5",
    "@nestjs/typeorm": "^9.0.1",
    "dayjs": "^1.11.10",
    "mysql2": "^3.9.8",
    "object-hash": "^3.0.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^4.3.1",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@johanblumenberg/ts-mockito": "^1.0.45",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/object-hash": "^2.2.1",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.4",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
